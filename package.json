{
  "name": "www-nurliman-dev",
  "version": "0.1.0",
  "private": true,
  "packageManager": "bun@1.3.5",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "changeset": "changeset",
    "lint-staged": "lint-staged",
    "format": "prettier --write --cache .",
    "prepare:turbo": "turbo telemetry disable",
    "prepare": "concurrently \"npm:prepare:*\""
  },
  "devDependencies": {
    "@changesets/changelog-git": "^0.2.1",
    "@changesets/cli": "^2.29.8",
    "@types/bun": "latest",
    "@types/node": "^24",
    "concurrently": "^9.2.1",
    "lefthook": "^2.0.13",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "prettier-plugin-sort-imports": "npm:@ianvs/prettier-plugin-sort-imports@^4.7.0",
    "prettier-plugin-svelte": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "turbo": "^2.7.3",
    "typescript": "^5.9.3"
  },
  "patchedDependencies": {
    "bigger-picture@1.1.19": "patches/bigger-picture@1.1.19.patch"
  },
  "trustedDependencies": [
    "@nurliman.dev/portfolio",
    "@tailwindcss/oxide",
    "esbuild",
    "lefthook"
  ],
  "lint-staged": {
    "**/*.+(js|ts|cjs|mjs|d.cts|d.mts|jsx|tsx|json|jsonc)": "prettier --write --cache",
    "**/*.+(astro|css|pcss|postcss|scss|html|md|svelte|yml|yaml)": "prettier --write --cache"
  }
}
