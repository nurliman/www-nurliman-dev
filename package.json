{
  "name": "www-nurliman-dev",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "lint-staged": "lint-staged",
    "format": "prettier --write \"**/*.+(js|mjs|cjs|jsx|ts|tsx|css|scss|md|json|yml|yaml)\" --ignore .gitignore --ignore .prettierignore",
    "format-staged": "pretty-quick --staged --ignore .gitignore --ignore .prettierignore",
    "prepare": "husky install"
  },
  "devDependencies": {
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "turbo": "^1.3.3"
  },
  "lint-staged": {
    "*.+(js|mjs|cjs|jsx|ts|tsx)": "eslint --cache --fix"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=v14.19.0"
  },
  "packageManager": "pnpm@7.33.6"
}
