{
  "name": "www-nurliman-dev",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "lint": "eslint ./src --ext .js,.mjs,.cjs,.jsx,.ts,.tsx --cache --fix",
    "format": "pretty-quick --staged --ignore .gitignore --ignore .prettierignore",
    "format:all": "prettier --write \"**/*.+(astro|js|mjs|cjs|jsx|ts|tsx|css|scss|md|json|yml|yaml)\" --ignore .gitignore --ignore .prettierignore",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nanostores/solid": "^0.1.6",
    "@solid-primitives/media": "^1.3.0",
    "clsx": "^1.1.1",
    "dayjs": "^1.11.1",
    "nanostores": "^0.5.12",
    "solid-js": "^1.3.17"
  },
  "devDependencies": {
    "@astrojs/solid-js": "^0.1.2",
    "@types/node": "^17.0.27",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "astro": "^1.0.0-beta.17",
    "autoprefixer": "^10.4.5",
    "eslint": "^8.14.0",
    "eslint-plugin-solid": "^0.4.7",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.0",
    "postcss": "^8.4.12",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.4.3",
    "prettier": "^2.6.2",
    "prettier-plugin-astro": "^0.0.12",
    "pretty-quick": "^3.1.3",
    "sass": "^1.51.0",
    "typescript": "^4.6.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.+(js|mjs|cjs|jsx|ts|tsx)": "eslint --cache --fix"
  }
}
