{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  },
  "globalDependencies": [
    ".env",
    "$SENTRY_DSN",
    "$NEXT_PUBLIC_SENTRY_DSN",
    "$NEXT_PUBLIC_GOOGLE_MAPS_API_KEY",
    "$NEXT_PUBLIC_RECAPTCHA_KEY"
  ]
}
